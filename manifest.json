{
    "manifest_version": 3,
    "name": "SpeAdy Gonzales",
    "version": "1.0.0",
    "description": "Speed up play rate of ad videos and skip as soon as possible on youtube",

    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },

    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["scripts/content.js"]
        }
      ]
}